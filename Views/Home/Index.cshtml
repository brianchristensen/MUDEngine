@{
    ViewBag.Title = "Ascendant Reverie";
}

<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8" />
        <title>@ViewBag.Title</title>

        <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
        <script src="~/Scripts/jquery-2.0.0.min.js" type="text/javascript"></script> 
        <script src="~/Scripts/jquery.signalR-1.0.1.js" type="text/javascript"></script>
        <script src="~/signalr/hubs" type="text/javascript"></script>
        <script src="~/Scripts/Mud.js" type="text/javascript"></script>   
    </head>
    <body>
        <div class="game"> 
            <div class="mud" id="mudWindow" onmousedown="setGlobalNoScroll()" onmouseup="setGlobalScroll()">
                <ul id="mudList" style="list-style: none"></ul>        
            </div>
            <div>
                <input type="text" class="command" id="tbCommand" style="color: #84BBFA; width: 720px; margin-left: 40px; margin-right: 40px;" autocomplete="off">
                <input type="button" id="btnInput" value="send" style="display: none">     
            </div>
        </div>             
    </body>
</html>
              